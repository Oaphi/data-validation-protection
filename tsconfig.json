{
    "compilerOptions": {
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
        "noEmit": true,
        "lib": ["ESNext"],
        "types": ["google-apps-script", "chai", "mocha", "node"],
        "target": "es2019",
        "moduleResolution": "node"
    },
    "include": ["src", "test"]
}
